<div class="bs-example" style=" margin-top: -20px">
    <div data-ride="carousel" class="carousel slide" id="carousel-example-captions">
        <ol class="carousel-indicators">
           {# <li class="" data-slide-to="0" data-target="#carousel-example-captions"></li>
            <li data-slide-to="1" data-target="#carousel-example-captions" class=""></li>
            <li data-slide-to="2" data-target="#carousel-example-captions" class=""></li>#}
        </ol>
        <div class="carousel-inner">
            {%for image  in service.getHomeSlideImage()%}
            <div class="item {%if loop.index==1%} active {%endif%}">
                <img alt="900x500" style="width:100%; height: 500px"  data-src="holder.js/100%x500/auto/#777:#777" src="{{asset(image.getWebPath())}}">
                <div class="carousel-caption" style="top: 0; left: 0; margin-top: 70px">
                    {{image.body |raw}}
                </div>
            </div>
            {%endfor%}
           {# <div class="item next left">
                <img alt="900x500" data-src="holder.js/100%x500/auto/#666:#666" src="data:image/svg+xml;base64,">
                <div class="carousel-caption">
                    <h3>Second slide label</h3>
                    <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit.</p>
                </div>
            </div>
            <div class="item">
                <img alt="900x500" data-src="holder.js/100%x500/auto/#555:#5555" src="data:image/svg+xml;base64,">
                <div class="carousel-caption">
                    <h3>Third slide label</h3>
                    <p>Praesent commodo cursus magna, vel scelerisque nisl consectetur.</p>
                </div>
            </div>#}
        </div>
        <a style="width:5%" data-slide="prev" href="#carousel-example-captions" class="left carousel-control">
            <span class="glyphicon glyphicon-chevron-left"></span>
        </a>
        <a style="width:5%" data-slide="next" href="#carousel-example-captions" class="right carousel-control">
            <span class="glyphicon glyphicon-chevron-right"></span>
        </a>
    </div>
</div>