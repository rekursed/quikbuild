{% extends 'ZeteqMarketBundle:Admin:admin_layout.html.twig' %}


{% block title %}
Admin
{% endblock %}
{% block content %}
    <h1>ProductSection list</h1>

 <table class="table table-hover table-responsive table-striped record_properties">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Enabled</th>
                <th>Description</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('productsection_edit', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.name }}</td>
                <td>{{ entity.enabled }}</td>
                <td>{{ entity.description }}</td>
                <td>
                <ul>
       
                    <li>
                        <a href="{{ path('productsection_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('productsection_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
